public class Main {
    public static void main(String args[]) {
      int x=10;
      int y=25;
      int z=9;
      int miCoche=1;

      System.out.println(x + y + z);
    }
    
    public static int suma (int x, int y, int z) {
        return z + y +z;
    }
    
class Coche{
    public int numeroPuertas = 4;
    }
    
    public static int Coche (int miCoche, int numeroPuertas) {
        
        int resultado = numeroPuertas + miCoche;
        System.out.println(resultado);
        return Coche(numeroPuertas, resultado);
      
    }

}
